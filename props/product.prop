# Blur
ro.surface_flinger.supports_background_blur=1
ro.sf.blurs_are_expensive=1

# IMS
ro.telephony.block_binder_thread_on_incoming_calls=false

# GFX Accel
ro.config.avoid_gfx_accel=true

# LMKD
ro.config.low_ram=false
ro.config.per_app_memcg=false
ro.lmk.log_stats=false
ro.lmk.use_psi=true
ro.lmk.use_minfree_levels=false
ro.lmk.low=1001
ro.lmk.medium=800
ro.lmk.critical=100
ro.lmk.critical_upgrade=false
ro.lmk.upgrade_pressure=100
ro.lmk.downgrade_pressure=100
ro.lmk.kill_heaviest_task=true
ro.lmk.kill_timeout_ms=0
ro.lmk.debug=false
ro.lmk.use_new_strategy=true
ro.lmk.psi_partial_stall_ms=100
ro.lmk.psi_complete_stall_ms=700
ro.lmk.thrashing_limit=100
ro.lmk.thrashing_limit_decay=10
ro.lmk.swap_util_max=100
ro.lmk.swap_free_low_percentage=5
ro.lmk.filecache_min_kb=0
ro.lmk.stall_limit_critical=100

# Volume steps
ro.config.alarm_vol_steps=15
ro.config.system_vol_steps=15
ro.config.media_vol_steps=15
ro.config.vc_call_vol_steps=15
